{
  "parser": "babel-eslint",
  "plugins": [
    "react", "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "settings": {
    "react": {
      "version": "16.4.1"
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "global-require": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "react/display-name": 0,
    "prettier/prettier": 2
  }
}
